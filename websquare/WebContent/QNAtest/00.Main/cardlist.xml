                ,startRowIndex: 7
                ,headerFontSize: "8"
                ,headerColor: "#CCE1EE"
                ,bodyFontSize: "8"
                ,bodyFontName: "굴림체"
                ,footerFontSize: "8"
                ,footerFontName: "굴림체"
                ,footerColor: "#FFF"
                ,colMerge: "true"
                ,printSet: {
                    landScape: "true"
                    ,fitToPage: "true"
//                    ,scale: "90"
                }
            }, infoArr);
        } catch (e) {
            
        }
	};
    
    scwin.trigger2_onclick = function(e) {
        dataList1.setJSON(JSON.parse(textarea1.getValue()));
    };
    
    scwin.onpageload();
	]]></script>
