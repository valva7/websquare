    Date.prototype.yyyymm = function(withDot) {
        var yyyy = this.getFullYear().toString();
        var mm = this.getMonth().toString();
        var dd = this.getDate().toString();
        if(withDot)
            return yyyy + ". "+(mm[1] ? mm : mm[0]==0 ? '12' : '0'+mm[0]);
        return yyyy +(mm[1] ? mm : mm[0]==0 ? '12' : '0'+mm[0]);
    }
    
    scwin.getInfoArr = function() {
        var infoArr = [];
        //경비 신청서/전표
        var infoObj = {
            rowIndex: 0
            ,colIndex: 0
            ,rowSpan: 1
            ,colSpan: 18
            ,text: "  경비 신청서/전표  ("+new Date().yyyymm(true)+")"
            ,textAlign: "center"
            ,fontSize: "15"
            ,drawBorder: "false"
        };
        infoArr.push(infoObj);
        
        var infoObj1 = {rowIndex: 0,colIndex: 0,rowSpan: 1,colSpan: 1,text: "",textAlign: "center",fontSize: "8",drawBorder: "true",borderColor: "#000",borderWidth: "medium",bgColor: "#CCE1EE"};
        var infoObj2 = {rowIndex: 0,colIndex: 0,rowSpan: 1,colSpan: 1,text: "",textAlign: "center",fontSize: "8",fontName: "굴림체",drawBorder: "true",borderColor: "#000",borderWidth: "medium",bgColor: "#FFF"};
        
        //우상단 기안자
        infoObj1.rowIndex = 2;
        infoObj1.colIndex = 10;
        infoObj1.rowSpan = 3;
        infoObj1.colSpan = 2;
        infoObj1.text = "소속본부";
        infoArr.push(infoObj1);
        infoObj1 = Object.assign({}, infoObj1);
        
        infoObj1.colIndex = 15;
        infoObj1.colSpan = 1;
        infoObj1.text = "경영지원";
        infoArr.push(infoObj1);
        infoObj1 = Object.assign({}, infoObj1);
        
        var colIdxArr = [12, 13, 14, 16, 17];
        var textArr = ["신청인", "팀장", "본부", "담당", "팀장"];
        infoObj1.rowSpan = 1;
        infoObj1.colSpan = 1;
        for(var i=0; i<colIdxArr.length; i++) {        
            infoObj1.colIndex = colIdxArr[i];
            infoObj1.text = textArr[i];
            infoArr.push(infoObj1);
            infoObj1 = Object.assign({}, infoObj1);
        }
        
        infoObj2.rowIndex = 3;
        infoObj2.colIndex = 12;
        infoObj2.rowSpan = 2;
        infoObj2.colSpan = 1;
        infoObj2.text = dataList1.getCellData(0, "sUseNm");
        infoArr.push(infoObj2);
        infoObj2 = Object.assign({}, infoObj2);
        
        var colIdxArr = [13, 14, 16, 17];
        var textArr = ["안현철", "송봉철", "진충경", "이종엽"];
        for(var i=0; i<colIdxArr.length; i++) {        
            infoObj2.colIndex = colIdxArr[i];
            infoObj2.text = textArr[i];
            infoArr.push(infoObj2);
            infoObj2 = Object.assign({}, infoObj2);
        }
        
        //하단 테이블
        var cnt = dataList1.getRowCount();
//        infoObj1.rowIndex = 13;
        infoObj1.rowIndex = cnt+10;
        infoObj1.colIndex = 0;
        infoObj1.rowSpan = 1;
        infoObj1.colSpan = 18;
        infoObj1.text = "경영지원팀 사용란";
        infoArr.push(infoObj1);
        infoObj1 = Object.assign({}, infoObj1);
        
        var colIdxArr = [0, 2, 7, 12, 15];
        var colSpanArr = [2, 5, 5, 3, 3];
        var textArr = ["계정코드", "계정과목", "부서명 및 코드", "금액", "비고"];
        
        for(var i=0; i<colIdxArr.length; i++) {
//            infoObj1.rowIndex = 14;
            infoObj1.rowIndex = cnt+11;
            infoObj1.colIndex = colIdxArr[i];
            infoObj1.colSpan = colSpanArr[i];
            infoObj1.text = textArr[i];
            infoArr.push(infoObj1);
            infoObj1 = Object.assign({}, infoObj1);
        }
        
        var colIdxArr = [0, 2, 7, 12, 15];
        var colSpanArr = [2, 5, 5, 3, 3];
        for(var i=0; i<3; i++) {
//            infoObj2.rowIndex = 15+i*2;
            infoObj2.rowIndex = (cnt+12)+i*2;
            infoObj2.rowSpan = 2;
            infoObj2.text = " ";
            for(var j=0; j<colIdxArr.length; j++) {
                infoObj2.colIndex = colIdxArr[j];
                infoObj2.colSpan = colSpanArr[j];
                infoArr.push(infoObj2);
                infoObj2 = Object.assign({}, infoObj2);
            }
        }
        
//        infoObj2.rowIndex = 21;
        infoObj2.rowIndex = cnt+18;
        infoObj2.colIndex = 0;
        infoObj2.rowSpan = 2;
        infoObj2.colSpan = 18;
        infoObj2.text = "전표 :";
        infoObj2.textAlign = "left";
        infoArr.push(infoObj2);
        
        return infoArr;
    };
    
	scwin.trigger1_onclick = function(e) {
